ShanH <- function(x = ""){
H = 0
for (n_1 in x){
if(n_1 > 0){
p = n_1 / sum(x)
H = H - p*log(p)
}
}
return(H)
}
ShanH(site1)
diversity(site1, index = "shannon")
SimpD <- function(x = ""){
D = 0
N = sum(x)
for (n_i in x){
D = D + (n_i^2)/(N^2)
}
return(D)
}
